{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/s/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/health",
      "dest": "api/index.py"
    },
    {
      "src": "/sw.js",
      "dest": "frontend/sw.js"
    },
    {
      "src": "/manifest.json",
      "dest": "frontend/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}
